   "source": [
    "n = 8\n",
    "for i in range(n):\n",
    "    print(i , end=' ')\n",
    "print()\n",
    "\n",
    "for i in range(0,n):\n",
    "    print(i , end=' ')\n",
    "print()\n",
    "\n",
    "for i in range(0,n,2):\n",
    "    print(i,end=' ')\n",
    "print()\n",
    "\n",
    "for i in range(n,0,-1):\n",
    "    print(i,end=\" \")\n",
    "print()\n"
   ]
